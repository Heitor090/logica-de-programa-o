programa
{
	
	funcao inicio()
	{
		inteiro numero
		escreva("Escreva um numero: ")
		leia(numero)

		se(numero>10){
			escreva("É MAIOR QUE 10!")
		}
		senao{
			escreva("NÃO É MAIOR QUE 10!")
		}
	}
}
