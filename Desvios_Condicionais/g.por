programa
{
	
	funcao inicio()
	{
		inteiro a, b
		escreva("Escreva o valor A: ")
		leia(a)
		escreva("Escreva o valor B: ")
		leia(b)

        se(a>b){
        	escreva("A ordem crescente é: ", a, " ",b)
        }
        senao{
        	escreva("A ordem crescente é: ", b," ", a)
        }
		
	}
}

